                            </el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                </el-col>\r\n\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='yonghu' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"用户姓名\" prop=\"yonghuName\">\r\n                        <el-input v-model=\"yonghuForm.yonghuName\"\r\n                                  placeholder=\"用户姓名\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"用户姓名\" prop=\"yonghuName\">\r\n                            <el-input v-model=\"ruleForm.yonghuName\"\r\n                                      placeholder=\"用户姓名\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\" v-if=\"sessionTable !='yonghu' \">\r\n                    <el-form-item class=\"upload\" v-if=\"type!='info' && !ro.yonghuPhoto\" label=\"头像\" prop=\"yonghuPhoto\">\r\n                        <img style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in (yonghuForm.yonghuPhoto || '').split(',')\" :src=\"item\" width=\"100\" height=\"100\">\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item v-if=\"ruleForm.yonghuPhoto\" label=\"头像\" prop=\"yonghuPhoto\">\r\n                            <img style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in (ruleForm.yonghuPhoto || '').split(',')\" :src=\"item\" width=\"100\" height=\"100\">\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='yonghu' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"联系方式\" prop=\"yonghuPhone\">\r\n                        <el-input v-model=\"yonghuForm.yonghuPhone\"\r\n                                  placeholder=\"联系方式\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"联系方式\" prop=\"yonghuPhone\">\r\n                            <el-input v-model=\"ruleForm.yonghuPhone\"\r\n                                      placeholder=\"联系方式\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='yonghu' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"用户身份证号\" prop=\"yonghuIdNumber\">\r\n                        <el-input v-model=\"yonghuForm.yonghuIdNumber\"\r\n                                  placeholder=\"用户身份证号\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"用户身份证号\" prop=\"yonghuIdNumber\">\r\n                            <el-input v-model=\"ruleForm.yonghuIdNumber\"\r\n                                      placeholder=\"用户身份证号\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='yonghu' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"邮箱\" prop=\"yonghuEmail\">\r\n                        <el-input v-model=\"yonghuForm.yonghuEmail\"\r\n                                  placeholder=\"邮箱\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"邮箱\" prop=\"yonghuEmail\">\r\n                            <el-input v-model=\"ruleForm.yonghuEmail\"\r\n                                      placeholder=\"邮箱\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='yonghu' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"部门\" prop=\"bumenValue\">\r\n                        <el-input v-model=\"yonghuForm.bumenValue\"\r\n                                  placeholder=\"部门\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"部门\" prop=\"bumenValue\">\r\n                            <el-input v-model=\"ruleForm.bumenValue\"\r\n                                      placeholder=\"部门\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='yonghu' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"岗位\" prop=\"gangweiValue\">\r\n                        <el-input v-model=\"yonghuForm.gangweiValue\"\r\n                                  placeholder=\"岗位\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"岗位\" prop=\"gangweiValue\">\r\n                            <el-input v-model=\"ruleForm.gangweiValue\"\r\n                                      placeholder=\"岗位\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <input id=\"updateId\" name=\"id\" type=\"hidden\">\r\n            <input id=\"yonghuId\" name=\"yonghuId\" type=\"hidden\">\r\n               <el-col :span=\"12\">\r\n                   <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"工作标题\" prop=\"gongzuoshenpiName\">\r\n                       <el-input v-model=\"ruleForm.gongzuoshenpiName\"\r\n                                 placeholder=\"工作标题\" clearable  :readonly=\"ro.gongzuoshenpiName\"></el-input>\r\n                   </el-form-item>\r\n                   <div v-else-if=\"type=='info'\">\r\n                       <el-form-item class=\"input\" label=\"工作标题\" prop=\"gongzuoshenpiName\">\r\n                           <el-input v-model=\"ruleForm.gongzuoshenpiName\"\r\n                                     placeholder=\"工作标题\" readonly></el-input>\r\n                       </el-form-item>\r\n                   </div>\r\n               </el-col>\r\n                <el-col :span=\"12\">\r\n                    <el-form-item class=\"select\" v-if=\"type!='info'\"  label=\"工作类型\" prop=\"gongzuoshenpiTypes\">\r\n                        <el-select v-model=\"ruleForm.gongzuoshenpiTypes\" :disabled=\"ro.gongzuoshenpiTypes\" placeholder=\"请选择工作类型\">\r\n                            <el-option\r\n                                v-for=\"(item,index) in gongzuoshenpiTypesOptions\"\r\n                                v-bind:key=\"item.codeIndex\"\r\n                                :label=\"item.indexName\"\r\n                                :value=\"item.codeIndex\">\r\n                            </el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"工作类型\" prop=\"gongzuoshenpiValue\">\r\n                        <el-input v-model=\"ruleForm.gongzuoshenpiValue\"\r\n                            placeholder=\"工作类型\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\">\r\n                    <el-form-item class=\"upload\" v-if=\"type!='info' && !ro.gongzuoshenpiFile\" label=\"工作文件\" prop=\"gongzuoshenpiFile\">\r\n                        <file-upload\r\n                                tip=\"点击上传工作文件\"\r\n                                action=\"file/upload\"\r\n                                :limit=\"3\"\r\n                                :multiple=\"true\"\r\n                                :fileUrls=\"ruleForm.gongzuoshenpiFile?ruleForm.gongzuoshenpiFile:''\"\r\n                                @change=\"gongzuoshenpiFileUploadChange\"\r\n                        ></file-upload>\r\n                    </el-form-item>\r\n                    <div v-else>\r\n                        <el-form-item v-if=\"ruleForm.gongzuoshenpiFile\" label=\"工作文件\" prop=\"gongzuoshenpiFile\">\r\n                            <a type=\"text\" style=\"text-decoration:none\" class=\"el-button\" :href=\"ruleForm.gongzuoshenpiFile\"  >工作文件下载</a>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"24\">\r\n                    <el-form-item v-if=\"type!='info'\"  label=\"工作详情\" prop=\"gongzuoshenpiText\">\r\n                        <el-input type=\"textarea\"  :readonly=\"ro.gongzuoshenpiText\" v-model=\"ruleForm.gongzuoshenpiText\" placeholder=\"工作详情\"></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item v-if=\"ruleForm.gongzuoshenpiText\" label=\"工作详情\" prop=\"gongzuoshenpiText\">\r\n                            <span v-html=\"ruleForm.gongzuoshenpiText\"></span>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n            <el-col :span=\"12\" v-if=\"type=='info'\">\r\n                <div>\r\n                    <el-form-item v-if=\"ruleForm.gongzuoshenpiYesnoTypes\" label=\"工作审批状态\" prop=\"gongzuoshenpiYesnoTypes\">\r\n                        <el-input v-model=\"ruleForm.gongzuoshenpiYesnoValue\" placeholder=\"工作审批状态\" readonly></el-input>\r\n                    </el-form-item>\r\n                </div>\r\n            </el-col>\r\n            <el-col :span=\"12\" v-if=\"type=='info'\">\r\n                <div>\r\n                    <el-form-item v-if=\"ruleForm.gongzuoshenpiYesnoText\" label=\"工作审批结果\" prop=\"gongzuoshenpiYesnoText\">\r\n                        <span v-html=\"ruleForm.gongzuoshenpiYesnoText\"></span>\r\n                    </el-form-item>\r\n                </div>\r\n            </el-col>\r\n            </el-row>\r\n            <el-form-item class=\"btn\">\r\n                <el-button v-if=\"type!='info'\" type=\"primary\" class=\"btn-success\" @click=\"onSubmit\">提交</el-button>\r\n                <el-button v-if=\"type!='info'\" class=\"btn-close\" @click=\"back()\">取消</el-button>\r\n                <el-button v-if=\"type=='info'\" class=\"btn-close\" @click=\"back()\">返回</el-button>\r\n            </el-form-item>\r\n        </el-form>\r\n    </div>\r\n</template>\r\n<script>\r\n    import styleJs from \"../../../utils/style.js\";\r\n    // 数字，邮件，手机，url，身份证校验\r\n    import { isNumber,isIntNumer,isEmail,isPhone, isMobile,isURL,checkIdCard } from \"@/utils/validate\";\r\n    export default {\r\n        data() {\r\n            return {\r\n                addEditForm:null,\r\n                id: '',\r\n                type: '',\r\n                sessionTable : \"\",//登录账户所在表名\r\n                role : \"\",//权限\r\n                userId:\"\",//当前登录人的id\r\n                yonghuForm: {},\r\n                ro:{\r\n                    yonghuId: false,\r\n                    gongzuoshenpiName: false,\r\n                    gongzuoshenpiTypes: false,\r\n                    gongzuoshenpiFile: false,\r\n                    gongzuoshenpiText: false,\r\n                    gongzuoshenpiYesnoTypes: false,\r\n                    gongzuoshenpiYesnoText: false,\r\n                },\r\n                ruleForm: {\r\n                    yonghuId: '',\r\n                    gongzuoshenpiName: '',\r\n                    gongzuoshenpiTypes: '',\r\n                    gongzuoshenpiFile: '',\r\n                    gongzuoshenpiText: '',\r\n                    gongzuoshenpiYesnoTypes: '',\r\n                    gongzuoshenpiYesnoText: '',\r\n                },\r\n                gongzuoshenpiTypesOptions : [],\r\n                gongzuoshenpiYesnoTypesOptions : [],\r\n                yonghuOptions : [],\r\n                rules: {\r\n                   yonghuId: [\r\n                              { required: true, message: '用户不能为空', trigger: 'blur' },\r\n                              {  pattern: /^[0-9]*$/,\r\n                                  message: '只允许输入整数',\r\n                                  trigger: 'blur'\r\n                              }\r\n                          ],\r\n                   gongzuoshenpiName: [\r\n                              { required: true, message: '工作标题不能为空', trigger: 'blur' },\r\n                          ],\r\n                   gongzuoshenpiTypes: [\r\n                              { required: true, message: '工作类型不能为空', trigger: 'blur' },\r\n                              {  pattern: /^[0-9]*$/,\r\n                                  message: '只允许输入整数',\r\n                                  trigger: 'blur'\r\n                              }\r\n                          ],\r\n                   gongzuoshenpiFile: [\r\n                              { required: true, message: '工作文件不能为空', trigger: 'blur' },\r\n                          ],\r\n                   gongzuoshenpiText: [\r\n                              { required: true, message: '工作详情不能为空', trigger: 'blur' },\r\n                          ],\r\n                   gongzuoshenpiYesnoTypes: [\r\n                              { required: true, message: '工作审批状态不能为空', trigger: 'blur' },\r\n                              {  pattern: /^[0-9]*$/,\r\n                                  message: '只允许输入整数',\r\n                                  trigger: 'blur'\r\n                              }\r\n                          ],\r\n                   gongzuoshenpiYesnoText: [\r\n                              { required: true, message: '工作审批结果不能为空', trigger: 'blur' },\r\n                          ],\r\n                }\r\n            };\r\n        },\r\n        props: [\"parent\"],\r\n        computed: {\r\n        },\r\n        created() {\r\n            //获取当前登录用户的信息\r\n            this.sessionTable = this.$storage.get(\"sessionTable\");\r\n            this.role = this.$storage.get(\"role\");\r\n            this.userId = this.$storage.get(\"userId\");\r\n\r\n\r\n            if (this.role != \"管理员\"){\r\n            }\r\n            this.addEditForm = styleJs.addStyle();\r\n            this.addEditStyleChange()\r\n            this.addEditUploadStyleChange()\r\n            //获取下拉框信息\r\n                this.$http({\r\n                    url:`dictionary/page?page=1&limit=100&sort=&order=&dicCode=gongzuoshenpi_types`,\r\n                    method: \"get\"\r\n                }).then(({ data }) => {\r\n                    if (data && data.code === 0) {\r\n                        this.gongzuoshenpiTypesOptions = data.data.list;\r\n                    }\r\n                });\r\n                this.$http({\r\n                    url:`dictionary/page?page=1&limit=100&sort=&order=&dicCode=gongzuoshenpi_yesno_types`,\r\n                    method: \"get\"\r\n                }).then(({ data }) => {\r\n                    if (data && data.code === 0) {\r\n                        this.gongzuoshenpiYesnoTypesOptions = data.data.list;\r\n                    }\r\n                });\r\n\r\n         this.$http({\r\n             url: `yonghu/page?page=1&limit=100`,\r\n             method: \"get\"\r\n         }).then(({ data }) => {\r\n             if (data && data.code === 0) {\r\n                this.yonghuOptions = data.data.list;\r\n            }\r\n         });\r\n\r\n        },\r\n        mounted() {\r\n        },\r\n        methods: {\r\n            // 下载\r\n            download(file){\r\n                window.open(`${file}`)\r\n            },\r\n            // 初始化\r\n            init(id,type) {\r\n                if (id) {\r\n                    this.id = id;\r\n                    this.type = type;\r\n                }\r\n                if(this.type=='info'||this.type=='else'){\r\n                    this.info(id);\r\n                }\r\n                // 获取用户信息\r\n                this.$http({\r\n                    url:`${this.$storage.get(\"sessionTable\")}/session`,\r\n                    method: \"get\"\r\n                }).then(({ data }) => {\r\n                    if (data && data.code === 0) {\r\n                        var json = data.data;\r\n                    } else {\r\n                        this.$message.error(data.msg);\r\n                    }\r\n                });\r\n            },\r\n            yonghuChange(id){\r\n                this.$http({\r\n                    url: `yonghu/info/`+id,\r\n                    method: \"get\"\r\n                }).then(({ data }) => {\r\n                    if (data && data.code === 0) {\r\n                        this.yonghuForm = data.data;\r\n                    }\r\n                });\r\n            },\r\n            // 多级联动参数\r\n            info(id) {\r\n                let _this =this;\r\n                _this.$http({\r\n                    url: `gongzuoshenpi/info/${id}`,\r\n                    method: 'get'\r\n                }).then(({ data }) => {\r\n                    if (data && data.code === 0) {\r\n                        _this.ruleForm = data.data;\r\n                        _this.yonghuChange(data.data.yonghuId)\r\n                    } else {\r\n                        _this.$message.error(data.msg);\r\n                    }\r\n                });\r\n            },\r\n            // 提交\r\n            onSubmit() {\r\n                this.$refs[\"ruleForm\"].vali